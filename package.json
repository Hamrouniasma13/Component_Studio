{
  "name": "component-studio",
  "version": "1.1.4",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "color": "^3.0.0",
    "date-fns": "^1.30.1",
    "emotion": "^9.2.6",
    "emotion-theming": "^10.0.10",
    "google-map-react": "^1.1.4",
    "keyboardjs": "^2.5.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react-color": "^2.14.1",
    "react-day-picker": "^7.3.0",
    "react-icons-kit": "^1.1.7",
    "react-pose": "^4.0.8",
    "react-progressive-image": "^0.6.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.2",
    "react-slick": "^0.23.1",
    "react-spring": "7.1.1",
    "react-use": "^8.1.2",
    "rebound": "^0.1.0",
    "recharts": "^1.1.0",
    "scriptjs": "^2.5.9",
    "slugify": "^1.3.4",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "studio-form": "^0.4.11",
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@svgr/cli": "^4.2.0",
    "core-js": "^2.5.7",
    "create-index": "^2.4.0",
    "css-loader": "^2.1.1",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "studio-form": "^0.4.10"
  },
  "scripts": {
    "now-build": "yarn docz build",
    "build": "babel --delete-dir-on-start --presets=@babel/preset-env,@babel/preset-react --plugins @babel/plugin-proposal-class-properties src --out-dir dist",
    "dev": "npm run build -- --watch",
    "icons": "npx @svgr/cli -d src/icons assets/icons --icon --no-dimensions && yarn create-index src/icons",
    "docz-dev": "yarn docz dev"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "'readonly'",
      "SharedArrayBuffer": "'readonly'"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-console": 1,
      "quotes": [
        1,
        "single"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2
    }
  }
}
